             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="InteticsTest_project_db">

        <class>com.latyshonak.dao.Entity.Users</class>
        <class>com.latyshonak.dao.Entity.Role</class>
        <class>com.latyshonak.dao.Entity.Images</class>
        <class>com.latyshonak.dao.Entity.Tags</class>

        <properties>
